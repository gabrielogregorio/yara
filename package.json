{
  "name": "yara",
  "version": "0.0.1-alfa",
  "homepage": "https://gabrielogregorio.github.io/yara",
  "private": false,
  "main": "build/index.js",
  "engines": {
    "node": ">=16.x"
  },
  "scripts": {
    "dev": "vite --host",
    "test": "vitest",
    "test:coverage": "vitest --coverage --watch=false",
    "check": "pnpm run prettier:format & pnpm run eslint:lint & pnpm run tsc:lint & pnpm run test:coverage",
    "e2e:headless": "start-server-and-test _starting_app http://0.0.0.0:5556 _headless",
    "e2e": "start-server-and-test _starting_app http://0.0.0.0:5556 _open",
    "tsc:lint": "tsc -p .",
    "eslint:lint": "eslint -c ./.eslintrc.json --ext '.tsx,.ts' 'src/**' --fix",
    "prettier:format": "prettier *.{ts,js,md,tsx,jsx} --write",
    "prettier:check": "prettier *.{ts,js,md,tsx,jsx} --check",
    "build": "vite build",
    "test:ui": "vitest --ui",
    "preview": "vite preview",
    "clear": "rm -rf build coverage",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deploy": "gh-pages -d build",
    "_starting_app": "vite --host --port=5556"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "cross-env": "^7.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.4.5",
    "remark-gfm": "^3.0.1",
    "tailwind-merge": "^1.14.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.13",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/coverage-c8": "^0.25.7",
    "@vitest/ui": "^0.25.7",
    "autoprefixer": "^10.4.13",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.30.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.17.0",
    "eslint-plugin-spellcheck": "^0.0.20",
    "eslint-plugin-storybook": "^0.6.13",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.2",
    "jsdom": "^20.0.3",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.19",
    "prettier": "^2.8.1",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^4.0.5",
    "vite-plugin-pwa": "^0.14.0",
    "vitest": "^0.25.7"
  }
}
